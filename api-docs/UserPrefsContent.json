{
    "name": "User Prefs for Content",
    "request": {
        "method": "GET",
        "headers": [
            {
                "Authorization": "Token SessionToken"
            }
        ],
        "endpoint": "/api/ds/users/v1/user/prefs/content",
        "queryParams": [
            {
                "name": "user",
                "type": "string",
                "validValues": "UUID based user id of the user",
                "defaultValue": null,
                "multipleOccurrence": "Not supported, only one will be picked up",
                "comment": "Lists the user preferences value for types of content"
            }
        ]
    },
    "response": {
        "body": {
            "webpage": 0.2,
            "interactive": 0.4,
            "video": 0.5,
            "text": 0.1,
            "audio": 0.25
        },
        "comment": ""
    }
}